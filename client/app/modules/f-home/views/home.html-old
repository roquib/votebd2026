<section class="election-timeline">
  <div class="container">
    <header class="ze-title2">
      <h2  lang="{{$root.locale.lang}}">{{'ELECTION RESULTS TIMELINE'|translate}}</h2>
      <p  lang="{{$root.locale.lang}}">{{'Up-to-date results of all the latest national and local elections'|translate}}</p>
    </header>
    <ul class="mjh-timeline">
      <li ng-repeat="cElection in currentElections">
        <!--{{cElection}}-->



        <!--even -->
        <div class="mjh-firstTimeline" ng-show="$even">
          <div class="mjh-timeline-left">
            <div>
              <span class="mjh-year"  lang="{{$root.locale.lang}}">{{cElection.electionDate|date:'yyyy'|translateNumber}}</span><br/>
              <span class="mjh-date"  lang="{{$root.locale.lang}}">{{cElection.electionDate|date:'MMMM'|translate}}-{{cElection.electionDate|date:'dd'|translateNumber}}</span>
            </div>
          </div>
          <div class="mjh-timeline-right">
            <article class="mjh-commonArticle">
              <h3><a ui-sref="front.electionResult.allElectionResult"  lang="{{$root.locale.lang}}"><span ng-show="$root.locale.lang != 'bn_BD'">{{cElection.currentElectionNameEn}}</span><span ng-show="$root.locale.lang == 'bn_BD'">{{cElection.currentElectionNameBn}}</span></a></h3>
              <p  lang="{{$root.locale.lang}}">{{cElection.electionDescBn}} </p>
            </article>
          </div>
        </div>




        <!-- odd -->
        <div class="mjh-secondTimeline" ng-show="$odd">
          <div class="mjh-timeline-right xtra">
            <article class="mjh-commonArticle">
              <h3><a ui-sref="front.electionResult.allElectionResult"  lang="{{$root.locale.lang}}"><span ng-show="$root.locale.lang != 'bn_BD'">{{cElection.currentElectionNameEn}}</span><span ng-show="$root.locale.lang == 'bn_BD'">{{cElection.currentElectionNameBn}}</span></a></h3>
              <p  lang="{{$root.locale.lang}}">{{cElection.electionDescBn}}</p>
            </article>
          </div>
          <div class="mjh-timeline-left extra">
            <div>
              <span class="mjh-year"  lang="{{$root.locale.lang}}">{{cElection.electionDate|date:'yyyy'|translateNumber}}</span><br/>
              <span class="mjh-date"  lang="{{$root.locale.lang}}">{{cElection.electionDate|date:'MMMM'|translate}}-{{cElection.electionDate|date:'dd'|translateNumber}}</span>
            </div>
          </div>
        </div>
      </li>




      <li class="timeline-showmore">
        <div class="mjh-showMore">
          <!--<a  ui-sref="front.electionResult.allElectionResult"  ng-click="showMoreCurrentElection()" lang="{{$root.locale.lang}}">show more</a>-->
          <a    ng-click="showMoreCurrentElection()" lang="{{$root.locale.lang}}">show more</a>
