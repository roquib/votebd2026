      <div class="col-sm-4">
        <div>
          <header class="mjh-other-concern-Profile">
            <h2  lang="{{$root.locale.lang}}">{{'Political Corruption'|translate}}</h2>
            <span class="rss-source"  lang="{{$root.locale.lang}}">Source: <a href="http://votebd.wordpress.com" target="_blank">votebd.wordpress.com</a></span>
          </header>
          <ul class="mjh-other-concern-Lists">
            <li ng-repeat="eachNews in politicalViolance">
              <figure class="mjh-other-concern-Img">
                <a href="{{eachNews.link}}"><img src="{{eachNews.thumbImage}}"></a>
              </figure>
              <div class="mjh-other-concern">
                <h3 class="mjh-Pname"><a href="{{eachNews.link}}" target="_blank"  lang="{{$root.locale.lang}}">{{eachNews.newsTitle}}</a></h3>
                <span  lang="{{$root.locale.lang}}">{{eachNews.publishedDate|date:"dd/MM/yyyy"}}</span><br/>
                <span  lang="{{$root.locale.lang}}">{{eachNews.category}}</span>
              </div>
            </li>
          </ul>
          <div class="mjh-more"><a href="http://votebd.wordpress.com" target="_blank"  lang="{{$root.locale.lang}}">&#x271A See More</a></div>
        </div>
      </div>
      <!--col-start-->
      <div class="col-sm-4">
        <header class="mjh-other-concern-Profile pprofile">
          <h2  lang="{{$root.locale.lang}}">{{'Featured Candidate'|translate}}</h2>
        </header>
        <div class="myWrapper">
          <ul class="mjh-other-concern-Lists">
            <li ng-repeat="item in featuredCandidates">
             <h4 class="mjh-Pname Pname mjhCandidatefeature" ui-sref="front.candidateSearch.affidavit({id:item.id})">
               <span  lang="{{$root.locale.lang}}">{{'Name'|translate}}
                 <span ng-show="$root.locale.lang != 'bn_BD'"> : {{item.personNameEn}}</span>
                 <span ng-show="$root.locale.lang == 'bn_BD'"> : {{item.personNameBn}}</span>
               </span>
             </h4>
             <p class="mjh-Pname Pname">
               <span  lang="{{$root.locale.lang}}">{{'Father/husband`s name'|translate}}
                <span ng-show="$root.locale.lang != 'bn_BD'"> : {{item.fatherNameEn}}</span>
                <span ng-show="$root.locale.lang == 'bn_BD'"> : {{item.fatherNameBn}}</span>
              </span>
             </p>
             <p class="mjh-Pname Pname">
               <span  lang="{{$root.locale.lang}}">
                 {{'address'|translate}} : {{item.addressBnAF}}
               </span>
             </p>
            </li>
          </ul>
        </div>
      </div><!--col-end-->
    </div>
  </div>
</section>


